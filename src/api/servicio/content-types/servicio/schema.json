    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "Servicio",
    "description": "Servicios ofrecidos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
