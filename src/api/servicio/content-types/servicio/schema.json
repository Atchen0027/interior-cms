    "descripcion": {
      "type": "text",
      "required": true
    },
    "icono": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "orden": {
      "type": "integer",
      "required": false
    },
    "sitio_web": {
      "type": "enumeration",
      "enum": ["interior-landing", "otro-cliente-1", "otro-cliente-2"],
      "default": "interior-landing",
      "required": false
    }
  }
}
